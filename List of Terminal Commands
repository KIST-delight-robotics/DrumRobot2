// PCAN-USB 포트 확인
sudo uhubctl

// 파이썬 가상환경 실행/종료
source magenta/magenta_env/bin/activate
deactivate

// 파이썬 테스트 실행
python3 magenta/script.py --record --path magenta/ --repeat 4 --bpm 96 --param 5 6 6 5 4 4 5 6 6 5 6 6

// 전자드럼 연결 확인
aconnect -l

// 아두이노 포트 확인
ls /dev/ttyACM*

// 다이나믹셀 포트 확인
ls /dev/ttyUSB*

// 다이나믹셀 usb 포트 latency 변경 
// 1. 현재 latency 확인
cat /sys/bus/usb-serial/devices/ttyUSB0/latency_timer
// 2. latency 수정
echo 1 | sudo tee /sys/bus/usb-serial/devices/ttyUSB0/latency_timer
// 3. 수정된 latency 확인
cat /sys/bus/usb-serial/devices/ttyUSB0/latency_timer
출력: 1